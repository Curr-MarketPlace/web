{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/components/leftbar/index.jsx", "app/components/rightbar/Topcreator.jsx", "components/Home/page.tsx", "app/components/searchbar/Searchbar.tsx", "app/Market/Button.jsx", "app/components/rightbar/SettingsModal.jsx", "app/components/leftbar/Mainitems.jsx", "app/Settings/Preferences/Colortheme.jsx", "components/mainSlider/index.tsx", "components/searchbar/ConnectWallet.jsx"],
  "exclude": ["node_modules"]
}
